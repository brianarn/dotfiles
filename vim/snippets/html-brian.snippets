# Brian Arnold's custom HTML snippets

# Goog CDN includes
snippet jqcdn
	<script ${1:type="text/javascript" }src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js"></script>
	${2}

snippet jquicdn
	<script ${1:type="text/javascript" }src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
	${2}

snippet dojocdn
	<script ${1:type="text/javascript" }src="http://ajax.googleapis.com/ajax/libs/dojo/1.5/dojo/dojo.xd.js"></script>
	${2}
